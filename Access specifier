class Data {
    private String name; 

    public void seter(String s) {
        name = s;
    }

    public String getter() {
        return name;
    }
}

public class Main {
    public static void main(String args[]) {
        Data d = new Data();
        d.seter("Java");
        String name = d.getter();
        System.out.println(name);
    }
}
